{% extends 'base.html.twig' %}

{% block title %}Payment{% endblock %}

{% block body %}
    <h1>Payment</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ payments.nom }}</td>
            </tr>
            <tr>
                <th>Amount</th>
                <td>{{ payments.amount }}</td>
            </tr>
            <tr>
                <th>WaletID</th>
                <td>{{ payments.waletID }}</td>
            </tr>
            <tr>
                <th>TransactionId</th>
                <td>{{ payments.transactionId }}</td>
            </tr>
             <tr>
                <th>ID</th>
                <td>{{ payments.id }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_payment_index') }}">back to list</a>

    <a href="{{ path('app_payment_edit', {'id': payments.id}) }}">edit</a>

    {{ include('payment/_delete_form.html.twig') }}
{% endblock %}
